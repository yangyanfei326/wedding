.caseShare{
  width:100%;
  height:120rpx;
  line-height:120rpx;
  text-align:center;
  font-size:14px;
}
.detailDescTitle{
  display:block;
  font-size:14px;
  margin:40rpx;
  color:#666
}
.detailDesc>view{
  margin:0 40rpx 40rpx;
  font-size:12px;
  color:#666
}
.detailDesc>view>span{
  margin-right:30rpx;
}
.detailDesc{
  text-align:left
}
.detailDesc button{
  width:180rpx;
  height:70rpx;
  margin:0 0 0 40rpx;
  background:rgb(237, 93, 41);
  color:#fff;
  font-size:12px;
}
/*服务案例*/
.index-enter{
  display: flex;
  padding:20rpx 42rpx;
  flex-wrap:wrap;
}
.index-enter .enter-item{
  width:47%;
  display:flex;
  justify-content: center;
  flex-direction:column;
  padding:10rpx;
  font-size:12px;
  color:#666;
  margin-bottom:15rpx;
}
.index-enter .enter-item text{
  margin:15rpx;
}
.index-enter image{
  width:320rpx;
  height:190rpx;
}



